+#include <sysdep.h>
+
+/* For __riscv_vector this file defines memchr.  */
+/* #ifndef __riscv_vector */
+# define memchr __memchr_vector
+# undef libc_hidden_builtin_def
+# define libc_hidden_builtin_def(a)
+/* #endif */
+
+	.p2align 6
+ENTRY (memchr)
+
+   zext.b	a1,a1
+
+.L_memchr_loop:
+   vsetvli	a3,a2,e8,m8,tu,mu
+   vle8ff.v	v0,(a0)
+   vmseq.vx	v8,v0,a1
+   csrr	a3,vl
+   vfirst.m	a4,v8
+   sub	a2,a2,a3
+   bgez	a4,.L_found
+   add	a0,a0,a3
+   bltu	zero,a2,.L_memchr_loop
+
+.L_found:
+   bltz	a4,.L_not_found
+   
+   add	a0,a0,a4
+   ret
+.L_not_found:   
+   add	a0,zero,zero
+   ret
+END (memchr)
+libc_hidden_builtin_def (memchr)
