+MULTILIB_DIRNAMES = rv64imafdc_zicsr_zifencei \
+rv64imafdc_zicsr_zifencei_zba_zbb_zbc_zbs \
+rv64imafdcv_zicsr_zifencei_zve32f_zve32x_zve64d_zve64f_zve64x_zvl128b_zvl32b_zvl64b \
