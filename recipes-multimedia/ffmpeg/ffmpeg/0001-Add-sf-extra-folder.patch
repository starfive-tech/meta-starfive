+    OMX_U32 nOutputPortIndex;     /**< The index of the output port (if any) using
+                                     this buffer */
+    OMX_U32 nInputPortIndex;      /**< The index of the input port (if any) using
+                                     this buffer */
+} OMX_BUFFERHEADERTYPE;
